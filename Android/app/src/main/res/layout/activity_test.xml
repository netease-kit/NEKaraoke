<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:background="@android:color/white"
    android:layout_height="match_parent">
    <Button
        android:id="@+id/createRoom"
        android:text="createRoom"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <Button
        android:id="@+id/endRoom"
        android:text="endRoom"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <Button
        android:id="@+id/joinRoom"
        android:text="joinRoom"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <Button
        android:id="@+id/leaveRoom"
        android:text="leaveRoom"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <Button
        android:id="@+id/getRoomList"
        android:text="getRoomList"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
</LinearLayout>